syntax = "proto3";

package msgids;

message LoginReq {
	uint64 udid = 1;
	string account = 2;
	string password = 3;
	uint64 id = 4;
}

message Account{
	uint64 id = 1;
	string name = 2;
	string password = 3;
	repeated uint64 role_id = 4;
}

message Role{
	uint64 id = 1;
	string name = 2;
	uint64 udid = 3;
	uint64 account_id = 4;
}

message LoginAck {
	uint64 udid = 1;
	repeated Role roles = 2; 
}